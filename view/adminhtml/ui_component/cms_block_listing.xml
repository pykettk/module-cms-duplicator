<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="duplicate">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">
                                Duplicate
                            </item>
                            <item name="message" xsi:type="string" translate="true">
                                Are you sure you want to duplicate selected blocks?
                            </item>
                        </item>
                        <item name="url" xsi:type="url" path="cms_duplicator/cms/massDuplicate"/>
                        <item name="type" xsi:type="string">duplicate</item>
                        <item name="label" xsi:type="string" translate="true">Duplicate</item>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>
